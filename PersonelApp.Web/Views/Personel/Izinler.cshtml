@model IzinViewModel

@if (TempData["Message"] != null)
{
    @section message {
    @await Html.PartialAsync("_message", (string)TempData["Message"])
}
}

<div class="row mt-3">
<div class="col-md-1">
</div>
<div class="col-md-10">
    
@foreach (var Izin in Model.Izinler)
{
    <div class="card mb-3 mt-3" style="max-width: auto;">
        <div class="row no-gutters">
            <div class="col-md-1">
            </div>
            <div class="col-md-11">
                <div class="card-body">
                    <h7 class="card-title">@Izin.PersonelId</h7><hr />
                    <h7 class="card-title">@Izin.BaslangicTarihi</h7><hr />
                    <h7 class="card-title">@Izin.BitisTarihi</h7><hr />
                    <h7 class="card-title">@Izin.Kullanilanizin</h7><hr />
                </div>
            </div>
        </div>
    </div>

}
</div>
<div class="col-md-1">
</div>
</div>